{
    "name": "Drolox mod",
    "description": "For fans of Drolox gaming and gamers everywhere",
    "developer":
    {
        "name": "Drolox"
    },
    "icons":
    {
        "512": "pepper.png"
    },
    "manifest_version": 3,
    "mod":
    {
        "license": "license.txt",
        "payload":
        {
            "background_music":
            [
                "music/track_1.mp3"
            ],
            "browser_sounds":
            {
                "CLICK":
                [
                    "sound/click.mp3"
                ],
                "FEATURE_SWITCH_OFF":
                [
                    "sound/feature_switch_off.mp3"
                ],
                "FEATURE_SWITCH_ON":
                [
                    "sound/feature_switch_on.mp3"
                ],
                "HOVER":
                [
                    "sound/hover.mp3"
                ],
                "HOVER_UP":
                [
                    "sound/hover.mp3"
                ],
                "IMPORTANT_CLICK":
                [
                    "sound/important_click.mp3"
                ],
                "LEVEL_UPGRADE":
                [
                    "sound/level_upgrade.mp3"
                ],
                "LIMITER_OFF":
                [
                    "sound/limiter_off.mp3"
                ],
                "LIMITER_ON":
                [
                    "sound/limiter_on.mp3"
                ],
                "SWITCH_TOGGLE":
                [
                    "sound/switch.mp3"
                ],
                "TAB_CLOSE":
                [
                    "sound/freddy_fazbear.mp3"
                ],
                "TAB_INSERT":
                [
                    "sound/new_tab.mp3"
                ],
                "TAB_SLASH":
                [
                    "sound/tab_slash.mp3"
                ]
            },
            "keyboard_sounds":
            {
                "TYPING_BACKSPACE":
                [
                    "keyboard/backspace2.mp3"
                ],
                "TYPING_ENTER":
                [
                    "keyboard/tiny_1.mp3"
                ],
                "TYPING_LETTER":
                [
                    "keyboard/letter_12.mp3",
                    "keyboard/letter_22.mp3",
                    "keyboard/letter_32.mp3"
                ],
                "TYPING_SPACE":
                [
                    "keyboard/space2.m4a"
                ]
            },
            "page_styles":
            [
                {
                    "css":
                    [
                        "webmodding/opera.css"
                    ],
                    "matches":
                    [
                        "https://*.opera.com/*"
                    ],
                    "css":
                    [
                        "webmodding/changecolors.css",
                        "webmodding/main.theme.css"
                    ],
                    "matches":
                    [
                        "https://*.discord.com/*"
                    ],
					"css":
                    [
                        "webmodding/changecolors.css",
                        "webmodding/main.theme.css"
                    ],
                    "matches":
                    [
                        "https://*.youtube.com/*"
                    ]
                }
            ],
            "shaders":
            {
                "Matrix green":
                {
                    "path": "shader/matrix.txt"
                },
                "Wave":
                {
                    "animation":
                    {
                        "duration": 60,
                        "steps": 3600
                    },
                    "path": "shader/wave.txt"
                }
            },
            "theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 200,
                        "s": 100,
                        "l": 55
                    },
                    "gx_secondary_base":
                    {
                        "h": 0,
                        "s": 0,
                        "l": 0
                    }
                },
                "light":
                {
                    "gx_accent":
                    {
                        "h": 300,
                        "s": 100,
                        "l": 45
                    },
                    "gx_secondary_base":
                    {
                        "h": 360,
                        "s": 100,
                        "l": 100
                    }
                }
            },
            "wallpaper":
            {
                "dark":
                {   
                    "image": "wallpaper/new_final_wallpaper_animation.mp4",
                    "first_frame": "wallpaper/wallpaper.png",
                    "text_color": "#ff00e6",
                    "text_shadow": "#470040"
                },
                "light":
                {
                    "image": "wallpaper/wallpaper.png",
                    "text_color": "#2ad4fe",
                    "text_shadow": "#2a94fe"
                }
            }
        },
        "schema_version": 1
    },
    "version": "2.3"
}
